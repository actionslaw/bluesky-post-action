{
  "name": "bluesky-post-action",
  "version": "1.0.1",
  "description": "Github Action to send posts for the AT Protocol",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc && npm run pack",
    "format": "prettier --write **/*.ts",
    "lint": "prettier --check **/*.ts",
    "pack": "esbuild lib/main.js --outfile=dist/index.js  --bundle --platform=node --target=node10.4",
    "all": "npm run build && npm run format && npm run lint"
  },
  "author": "Ric Wood",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rg-wood/atproto-post-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "social",
    "bluesky",
    "atprotocol"
  ],
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@atproto/api": "^0.7.4",
    "node-fetch": "^3.3.2",
    "nvm": "^0.0.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "esbuild": "0.19.9",
    "prettier": "^3.1.1",
    "typescript": "^4.8.4"
  }
}
